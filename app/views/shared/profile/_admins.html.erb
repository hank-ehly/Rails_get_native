<div class="shared profile admins col-md-6 col-sm-7">
	<h2><strong>Admins</strong></h2>
	<hr>
	<%= link_to "+ Add Admin", controller: 'admins', action: 'add' %>
	<br>
	<br>
	<table class="table table-hover">
		<tr>
			<th>Name</th>
			<th>Id</th>
			<th>Email</th>
		</tr>
	<% @admins.each do |admin| %>
		<tr>
			<td><%= full_name_of(admin) %></td>
			<td><%= admin.id %></td>
			<td><%= admin.email %></td>
		</tr>
	<% end %>
	</table>
</div>